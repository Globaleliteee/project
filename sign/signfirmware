BINCTL=./tools/binctl
KEYCTL=./tools/keyctl

TARGET=../build/firmware/firmware.bin

# copy this file to signfirmware
fwseckey1=5454545454545454545454545454545454545454545454545454545454543131
fwseckey2=5454545454545454545454545454545454545454545436545454545454545432
fwseckey3=5454545454545454545454545454545454545454545454545454545454547133

$BINCTL $TARGET -h
$BINCTL $TARGET -s 1:2 `$KEYCTL sign firmware $TARGET $fwseckey1 $fwseckey2`
